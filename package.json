{
  "name": "sam-ai-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "verify:tenant-isolation": "node scripts/js/verify-tenant-isolation.mjs",
    "build": "npm run verify:tenant-isolation && next build",
    "build:staging": "cp .env.staging .env.local.bak && mv .env.local .env.local.prod && mv .env.staging .env.local && next build && mv .env.local .env.staging && mv .env.local.prod .env.local",
    "start": "next start",
    "lint": "next lint",
    "deploy:staging": "npm run build:staging && netlify deploy --dir=.next --alias=staging",
    "deploy:production": "bash scripts/shell/deploy-all-tenants.sh",
    "deploy:all-tenants": "bash scripts/shell/deploy-all-tenants.sh",
    "validate:staging": "node docs/scripts/deployment/staging-validation.js",
    "backup:create": "node docs/scripts/deployment/backup-rollback.js backup",
    "backup:list": "node docs/scripts/deployment/backup-rollback.js list",
    "backup:rollback": "node docs/scripts/deployment/backup-rollback.js rollback",
    "backup:cleanup": "node docs/scripts/deployment/backup-rollback.js cleanup",
    "monitoring:health": "curl -s https://app.meet-sam.com/api/monitoring/health | jq .",
    "monitoring:metrics": "curl -s https://app.meet-sam.com/api/monitoring/metrics | jq .",
    "monitoring:alerts": "curl -s https://app.meet-sam.com/api/monitoring/alerts | jq .",
    "monitoring:workspace-integrity": "npx tsx scripts/monitor-workspace-integrity.ts",
    "monitoring:workspace-integrity:shell": "./scripts/monitor-workspace-integrity.sh",
    "test:integration": "node test-integration.js",
    "test:email": "node test-complete-flow.js",
    "test:kb-crud": "node tests/knowledge-base-crud.test.js",
    "test:sam-threads": "node tests/integration-sam-threads.test.js",
    "seed:kb-simple": "node scripts/seed-structured-kb-simple.js",
    "update-docs": "node scripts/js/auto-update-documentation.cjs",
    "post-deploy": "npm run update-docs && echo 'Documentation updated automatically'",
    "seed:demo": "node scripts/seed-demo-knowledge-base.js",
    "migrate:kb": "node scripts/migrate-legacy-knowledge-base.js",
    "fix-workspaces": "npx tsx scripts/fix-user-workspaces.ts",
    "check-workspaces": "npx tsx scripts/fix-user-workspaces.ts",
    "pre-commit": "npm run lint && npm run update-docs"
  },
